<GuiXml>
    <Controls>
        <TopLevelControl name="CFMUtilsAS" resizeToFitDescendents="true" clampedToScreen="true" mouseEnabled="true" movable="true">
            <Dimensions x="64" y="64" />
            <Anchor point="TOPLEFT" relativeTo="GuiRoot" relativePoint="CENTER" offsetX="0" offsetY="0"/>
            <OnMoveStop>AS.OnCFMUtilsASMove()</OnMoveStop>
                <Controls>
                     <!-- Llothis Tracker -->
                    <Label name="$(parent)Label1" font="$(BOLD_FONT)|$(KB_18)|soft-shadow-thick" color="ffffff" text="Saint Llothis the Pious:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85" hidden="true">			
                        <Dimensions x="180" y="30" />
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="0"/>
                    </Label>
                    <Label name="$(parent)Label1Value1" font="$(CHAT_FONT)|$(KB_18)|soft-shadow-thick" color="ffffff" text="100" verticalAlignment="CENTER" horizontalAlignment="RIGHT"  alpha="0.85" hidden="true">			
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)Label1" relativePoint="BOTTOMRIGHT" offsetY="-2"/>
                    </Label>
                    <Label name="$(parent)Label1Value2" font="$(CHAT_FONT)|$(KB_18)|soft-shadow-thick" color="ff0000" text="0" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85" hidden="true">			
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)Label1Value1" relativePoint="BOTTOMRIGHT" offsetX="5"/>
                    </Label> 
                    <Label name="$(parent)Label1Value3" font="$(CHAT_FONT)|$(KB_18)|soft-shadow-thick" color="ffffff" text="3:00" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85" hidden="true">			
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)Label1Value2" relativePoint="BOTTOMRIGHT" offsetX="5"/>
                    </Label>

                    <!-- Felms Tracker -->
                    <Label name="$(parent)Label2" font="$(BOLD_FONT)|$(KB_18)|soft-shadow-thick" color="ffffff" text="Saint Felms the Bold:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85" hidden="true">			
                        <Dimensions x="180" y="30" />
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="30"/>
                    </Label>
                    <Label name="$(parent)Label2Value1" font="$(CHAT_FONT)|$(KB_18)|soft-shadow-thick" color="ffffff" text="100" verticalAlignment="CENTER" horizontalAlignment="RIGHT"  alpha="0.85" hidden="true">			
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)Label2" relativePoint="BOTTOMRIGHT" offsetY="-2"/>
                    </Label>
                    <Label name="$(parent)Label2Value2" font="$(CHAT_FONT)|$(KB_18)|soft-shadow-thick" color="ff0000" text="0" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85" hidden="true">			
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)Label2Value1" relativePoint="BOTTOMRIGHT" offsetX="5"/>
                    </Label> 
                    <Label name="$(parent)Label2Value3" font="$(CHAT_FONT)|$(KB_18)|soft-shadow-thick" color="ffffff" text="3:00" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85" hidden="true">			
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)Label2Value2" relativePoint="BOTTOMRIGHT" offsetX="5"/>
                    </Label>                 
                </Controls>
        </TopLevelControl>
        <TopLevelControl name="CFMUtilsASDebug" resizeToFitDescendents="true" clampedToScreen="true" mouseEnabled="false" movable="false">
            <Anchor point="TOPLEFT" relativeTo="GuiRoot" relativePoint="CENTER" offsetX="450" offsetY="-600"/>
                <Controls>
                     <!-- Date/Time -->
                    <Label name="$(parent)Label1" font="$(BOLD_FONT)|$(KB_26)|soft-shadow-thick" color="ffffff" text="Date And Time" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85" hidden="true">			
                        <Dimensions x="300" y="30" />
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="0"/>
                    </Label>
                    <!-- Protector Target ID -->
                    <Label name="$(parent)Label2" font="$(BOLD_FONT)|$(KB_26)|soft-shadow-thick" color="ffffff" text="Protector Target ID" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85" hidden="true">			
                        <Dimensions x="300" y="30" />
                        <Anchor point="BOTTOMLEFT"  relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="30"/>
                    </Label>                 
                </Controls>            
        </TopLevelControl>
    </Controls>
</GuiXml>